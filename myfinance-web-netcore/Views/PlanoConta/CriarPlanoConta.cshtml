@model PlanoContaModel

@{
    ViewData["Title"] = "Plano de Conta";
}

<h1>@ViewData["Title"]</h1>

<form asp-controller="PlanoConta" asp-action="CriarPlanoConta">

  <label for="fname">Descrição:</label><br>
  <input type="text" asp-for="Descricao"><br>

  <label for="lname">Tipo:</label><br>
  <input type="text" asp-for="Tipo"><br>

  <button type="submit" class="btn btn-block btn-success">Registrar</button>
</form>